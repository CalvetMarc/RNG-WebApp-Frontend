{
  "navigationFallback": { "rewrite": "/index.html" },
  "mimeTypes": { ".wasm": "application/wasm" },
  "responseOverrides": [
    {
      "route": "/index.html",
      "headers": { "Cache-Control": "no-store" }
    },
    {
      "extension": ".wasm",
      "headers": { "Cache-Control": "public, max-age=2592000, immutable" }
    }
  ]
}
