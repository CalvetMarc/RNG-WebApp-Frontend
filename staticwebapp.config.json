{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js", "/*.mjs", "/*.css", "/*.map",
      "/*.png", "/*.jpg", "/*.jpeg", "/*.gif", "/*.svg", "/*.ico",
      "/*.json", "/*.wasm"
    ]
  },
  "mimeTypes": {
    ".js":  "application/javascript",
    ".mjs": "application/javascript",
    ".css": "text/css",
    ".map": "application/json",
    ".json":"application/json",
    ".wasm":"application/wasm"
  }
}
